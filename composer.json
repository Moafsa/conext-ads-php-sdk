{
  "name": "conext/ads-sdk",
  "description": "Official PHP SDK for Conext Ads API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Conext\\Ads\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Conext\\Ads\\Tests\\": "tests/"
    }
  }
}